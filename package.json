{
	"name": "eth-abi-interactive",
	"displayName": "Eth ABI Interactive",
	"publisher": "jlcs-es",
	"description": "Interact with deployed Ethereum smart contracts using the ABI",
	"license": "SEE LICENSE IN LICENSE.txt",
	"icon": "media/eth.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/jlcs-es/eth-abi-interactive"
	},
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:eth-abi-interactive.contracts"
	],
	"extensionDependencies": [
		"7finney.ethcode"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "Ethereum ABI Interactive"
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "eth-abi-interactive-container",
					"title": "Eth ABI Interactive",
					"icon": "media/eth.png"
				}
			]
		},
		"views": {
			"eth-abi-interactive-container": [
				{
					"id": "eth-abi-interactive.contracts",
					"name": "Contracts"
				},
				{
					"id": "eth-abi-interactive.abis",
					"name": "ABI",
					"visibility": "collapsed"
				},
				{
					"id": "eth-abi-interactive.pendingTxn",
					"name": "Pending Transactions"
				}
			]
		},
		"commands": [
			{
				"command": "eth-abi-interactive.setEndpoint",
				"title": "Set Eth Endpoint",
				"icon": "$(plug)"
			},
			{
				"command": "eth-abi-interactive.refreshTreeView",
				"title": "Stop",
				"icon": "$(debug-pause)"
			},
			{
				"command": "eth-abi-interactive.useContract",
				"title": "Use Contract"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "eth-abi-interactive.refreshEntry",
					"when": "view == eth-abi-interactive.contracts",
					"group": "navigation"
				},
				{
					"command": "eth-abi-interactive.refreshTreeView",
					"when": "view == eth-abi-interactive.pendingTxn",
					"group": "navigation"
				},
				{
					"command": "eth-abi-interactive.setEndpoint",
					"when": "view == eth-abi-interactive.contracts",
					"group": "navigation"
				},
				{
					"command": "eth-abi-interactive.addPrivateKey",
					"when": "view == eth-abi-interactive.accounts",
					"group": "navigation"
				},
				{
					"command": "eth-abi-interactive.refreshAccounts",
					"when": "view == eth-abi-interactive.accounts",
					"group": "navigation"
				}
			],
			"view/item/context": []
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.50.0",
		"@types/ws": "^8.5.4",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"alchemy-sdk": "^2.6.1",
		"bnc-sdk": "^4.6.7",
		"web3": "^1.3.0",
		"ws": "^8.12.1"
	}
}
