{
  "name": "eth-abi-interactive",
  "displayName": "Eth ABI Interactive",
  "publisher": "7finney",
  "description": "Smart Contracts ABI Interactive Interface",
  "license": "MIT",
  "icon": "media/eth.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/7finney/eth-abi-interactive"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "extensionDependencies": [
    "7finney.ethcode"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Ethereum ABI Interactive"
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "eth-abi-interactive-container",
          "title": "Eth ABI Interactive",
          "icon": "media/eth.png"
        }
      ]
    },
    "views": {
      "eth-abi-interactive-container": [
        {
          "id": "eth-abi-interactive.contracts",
          "name": "Contracts"
        },
        {
          "id": "eth-abi-interactive.abis",
          "name": "ABI",
          "visibility": "collapsed"
        },
        {
          "id": "eth-abi-interactive.pendingTxn",
          "name": "Pending Transactions"
        }
      ]
    },
    "commands": [
      {
        "command": "eth-abi-interactive.refreshContracts",
        "title": "Refresh",
        "icon": "$(extensions-refresh)"
      },
      {
        "command": "eth-abi-interactive.refreshTreeView",
        "title": "Stop",
        "icon": "$(debug-pause)"
      },
      {
        "command": "eth-abi-interactive.editInput",
        "title": "Edit input value",
        "icon": "$(edit)"
      },
      {
        "command": "eth-abi-interactive.sendTransaction",
        "title": "Send transaction",
        "icon": "$(play)"
      },
      {
        "command": "eth-abi-interactive.callContract",
        "title": "Call contract",
        "icon": "$(play)"
      },
      {
        "command": "eth-abi-interactive.deployContract",
        "title": "Deploy contract",
        "icon": "$(play)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "eth-abi-interactive.refreshTreeView",
          "when": "view == eth-abi-interactive.pendingTxn",
          "group": "navigation"
        },
        {
          "command": "eth-abi-interactive.refreshContracts",
          "when": "view == eth-abi-interactive.contracts",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "eth-abi-interactive.editInput",
          "when": "view == eth-abi-interactive.abis && viewItem == abiInput",
          "group": "inline"
        },
        {
          "command": "eth-abi-interactive.deployContract",
          "when": "view == eth-abi-interactive.contracts && viewItem == contract",
          "group": "inline"
        },
        {
          "command": "eth-abi-interactive.sendTransaction",
          "when": "view == eth-abi-interactive.abis && viewItem == abiFunction",
          "group": "inline"
        },
        {
          "command": "eth-abi-interactive.callContract",
          "when": "view == eth-abi-interactive.abis && viewItem == abiReadFunction",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.50.0",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "bnc-sdk": "^4.6.7",
    "ethers": "5.4.0",
    "ws": "^8.12.1"
  }
}
